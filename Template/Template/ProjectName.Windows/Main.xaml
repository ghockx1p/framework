<Window x:Class="$custommessage$.Windows.Main"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:d="clr-namespace:$custommessage$.Entities;assembly=$custommessage$.Entities"
    Title="$custommessage$ Application"  SizeToContent="WidthAndHeight">
    <Menu MaxHeight ="{x:Static SystemParameters.MenuBarHeight}" x:Name="menu">
        <MenuItem Header="Entities">
            <MenuItem Header="_$custommessage$" Tag="{m:Explore QueryName={x:Type d:MyEntityDN}}"/>
        </MenuItem>
    </Menu>
</Window>
